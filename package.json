{
  "name": "react-redux-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean:www": "rimraf www/*",
    "clean": "npm run clean:www",
    "copy:html": "mkdir -p www && cp -r src/index.html www/",
    "copy:css": "mkdir -p www/assets/css && cp -r src/css/* www/assets/css/",
    "copy:redirects": "mkdir -p www && cp -r src/_redirects www/",
    "copy": "npm run copy:html && npm run copy:css",
    "copy:staging": "npm run copy:html && npm run copy:css && npm run copy:redirects",
    "build:development": "npm run clean && npm run copy && webpack",
    "build:production": "npm run clean && npm run copy && webpack --config webpack.config.production.js",
    "build:staging": "npm run clean && npm run copy:staging && webpack --config webpack.config.staging.js",
    "build": "npm run build:development",
    "watch:html": "cpx --watch src/index.html www",
    "watch:webpack": "webpack --watch",
    "watch:browser": "browser-sync start --config bs-config.js",
    "watch": "npm run watch:html & npm run watch:webpack & npm run watch:browser",
    "start": "npm run build && npm run watch",
    "eslint": "eslint ./src; exit 0;",
    "stylelint": "stylelint ./src; exit 0;",
    "lint": "npm run eslint && npm run stylelint",
    "jest": "jest --verbose --coverage",
    "test": "npm run lint && npm run jest",
    "tdd": "npm run jest -- --watch"
  },
  "dependencies": {
    "history": "^4.7.2",
    "moducks": "^0.2.0-rc.3",
    "react": "^15.6.1",
    "react-css-modules": "^4.7.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.13",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "jest": "^20.0.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^2.1.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "testRegex": "__tests__/.*\\.(test|spec)\\.(tsx?|jsx?)$",
    "setupFiles": []
  }
}
